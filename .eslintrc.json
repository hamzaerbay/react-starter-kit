{
    "extends": "airbnb",
    "env": {
      "browser": true
    },
    "rules": {
       "semi": ["error", "always"],
       "quotes": ["error", "single"],
       "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
       "import/no-unresolved": 0,
       "no-named-as-default": 0
   }
}
